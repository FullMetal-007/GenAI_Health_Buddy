# .env file for Whatsapp Bot
# backend/.env

# Your WhatsApp Phone Number ID from the Meta App Dashboard
META_PHONE_NUMBER_ID=10xxxxxxxxxxxxx

# Your Temporary (or Permanent) Access Token
META_ACCESS_TOKEN=EAAD...

# The exact name of your approved message template
META_TEMPLATE_NAME=prescription_summary

# (Optional) The Graph API version, e.g., v19.0
META_API_VERSION=v19.0